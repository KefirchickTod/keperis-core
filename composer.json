{
    "name": "keperis/core-fr",
    "description": "Crm/Erp framework for render table and filters it's",
    "license": "MIT",
    "type": "libary",
    "authors": [
        {
            "name": "Zakhar Pylypchuck",
            "email": "zacharpu2@gmail.com"
        }
    ],
    "keywords": [
        "keperis",
        "core"
    ],
    "require": {
        "php": "^7.3.0",
        "doctrine/annotations": "^1.13",
        "illuminate/pagination": "8.x-dev",
        "nikic/fast-route": "^1.3",
        "phpoffice/phpspreadsheet": "^1.19",
        "pimple/pimple": "^3.2",
        "spatie/laravel-query-builder": "^4.0",
        "symfony/event-dispatcher": "^5.3",
        "symfony/http-foundation": "^5.3.10",
        "symfony/http-kernel": "^5.3.10",
        "symfony/routing": "5.4.x-dev",
        "vlucas/phpdotenv": "^5.2"
    },
    "require-dev": {
        "phpunit/phpunit": "9",
        "roave/security-advisories": "dev-master",
        "squizlabs/php_codesniffer": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Keperis\\": "Keperis/"
        },
        "files": [
            "Keperis/bootstrap.php"
        ]
    },
    "config": {
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "scripts": {
        "test": "phpunit test",
        "check-style": "phpcs Keperis tests",
        "fix-style": "phpcbf Keperis tests"
    },
    "minimum-stability": "dev"
}
